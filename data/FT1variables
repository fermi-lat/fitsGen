RA                      FT1Ra
DEC                     FT1Dec
#L                       FT1L
#B                       FT1B
THETA                   FT1Theta
PHI                     FT1Phi
ZENITH_ANGLE            FT1ZenithTheta
EARTH_AZIMUTH_ANGLE     FT1EarthAzimuth
TIME                    EvtElapsedTime
EVENT_ID                FT1EventId
#RECON_VERSION
#CALIB_VERSION
#EVENT_CLASS
#CONVERSION_TYPE
